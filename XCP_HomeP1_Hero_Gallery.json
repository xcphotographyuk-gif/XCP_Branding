{
  "version": "2.0",
  "title": "XC Photography Homepage - Part 1: Hero & Gallery Bar",
  "type": "page",
  "content": [
    {
      "id": "hero-section",
      "elType": "section",
      "settings": {
        "layout": "full_width",
        "gap": "no",
        "height": "min-height",
        "custom_height": {
          "unit": "vh",
          "size": 75
        },
        "background_background": "classic",
        "background_image": {
          "url": "assets/images/hero-woman-hair.jpg",
          "id": ""
        },
        "background_position": "center center",
        "background_attachment": "scroll",
        "background_repeat": "no-repeat",
        "background_size": "cover",
        "background_overlay_background": "gradient",
        "background_overlay_color": "rgba(0,0,0,0.4)",
        "background_overlay_color_b": "rgba(0,0,0,0.7)",
        "background_overlay_gradient_angle": {
          "unit": "deg",
          "size": 180
        }
      },
      "elements": [
        {
          "id": "hero-column",
          "elType": "column",
          "settings": {
            "_column_size": 100,
            "content_position": "middle"
          },
          "elements": [
            {
              "id": "hero-heading",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Branding & Documentary Photography That Demands Attention",
                "align": "center",
                "header_size": "h1"
              }
            },
            {
              "id": "hero-subheading",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Iconic Imagery for Iconic Brands with an Edge",
                "align": "center",
                "header_size": "h2"
              }
            },
            {
              "id": "hero-button-1",
              "elType": "widget",
              "widgetType": "button",
              "settings": {
                "text": "Book Your Free Consultation",
                "link": {
                  "url": "#consultation"
                },
                "align": "center"
              }
            },
            {
              "id": "hero-button-2",
              "elType": "widget",
              "widgetType": "button",
              "settings": {
                "text": "View Portfolio",
                "link": {
                  "url": "#portfolio"
                },
                "align": "center",
                "button_type": "default"
              }
            }
          ]
        }
      ]
    },
    {
      "id": "gallery-bar-section",
      "elType": "section",
      "settings": {
        "layout": "full_width",
        "gap": "no",
        "background_background": "classic",
        "background_color": "#2a2a2a"
      },
      "elements": [
        {
          "id": "gallery-col-1",
          "elType": "column",
          "settings": {
            "_column_size": 25,
            "background_background": "classic",
            "background_image": {
              "url": "assets/images/gallery-1.jpg"
            },
            "background_size": "cover",
            "background_position": "center center",
            "min_height": {
              "unit": "px",
              "size": 400
            }
          },
          "elements": [
            {
              "id": "gallery-heading-1",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Urban Edge",
                "align": "left",
                "header_size": "h3"
              }
            }
          ]
        },
        {
          "id": "gallery-col-2",
          "elType": "column",
          "settings": {
            "_column_size": 25,
            "background_background": "classic",
            "background_image": {
              "url": "assets/images/gallery-2.jpg"
            },
            "background_size": "cover",
            "background_position": "center center",
            "min_height": {
              "unit": "px",
              "size": 400
            }
          },
          "elements": [
            {
              "id": "gallery-heading-2",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Raw Emotion",
                "align": "left",
                "header_size": "h3"
              }
            }
          ]
        },
        {
          "id": "gallery-col-3",
          "elType": "column",
          "settings": {
            "_column_size": 25,
            "background_background": "classic",
            "background_image": {
              "url": "assets/images/gallery-3.jpg"
            },
            "background_size": "cover",
            "background_position": "center center",
            "min_height": {
              "unit": "px",
              "size": 400
            }
          },
          "elements": [
            {
              "id": "gallery-heading-3",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Bold Stories",
                "align": "left",
                "header_size": "h3"
              }
            }
          ]
        },
        {
          "id": "gallery-col-4",
          "elType": "column",
          "settings": {
            "_column_size": 25,
            "background_background": "classic",
            "background_image": {
              "url": "assets/images/gallery-4.jpg"
            },
            "background_size": "cover",
            "background_position": "center center",
            "min_height": {
              "unit": "px",
              "size": 400
            }
          },
          "elements": [
            {
              "id": "gallery-heading-4",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Timeless Moments",
                "align": "left",
                "header_size": "h3"
              }
            }
          ]
        }
      ]
    }
  ]
}
