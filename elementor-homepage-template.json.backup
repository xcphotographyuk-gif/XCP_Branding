{
  "version": "2.0",
  "title": "XC Photography Homepage",
  "type": "page",
  "content": [
    {
      "id": "hero-section",
      "elType": "section",
      "settings": {
        "layout": "full_width",
        "gap": "no",
        "height": "min-height",
        "custom_height": {
          "unit": "vh",
          "size": 100
        },
        "background_background": "classic",
        "background_image": {
          "url": "https://via.placeholder.com/1920x1080/1a1a1a/ffffff?text=Cinematic+Background",
          "id": ""
        },
        "background_position": "center center",
        "background_size": "cover",
        "background_overlay_background": "gradient",
        "background_overlay_color": "rgba(0,0,0,0.4)",
        "background_overlay_color_b": "rgba(0,0,0,0.7)",
        "background_overlay_gradient_angle": {
          "unit": "deg",
          "size": 180
        }
      },
      "elements": [
        {
          "id": "hero-column",
          "elType": "column",
          "settings": {
            "width": "100",
            "_column_size": 100,
            "content_position": "middle"
          },
          "elements": [
            {
              "id": "hero-heading",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Branding & Documentary Photography That Demands Attention",
                "align": "center",
                "header_size": "h1",
                "title_color": "#ffffff",
                "typography_typography": "custom",
                "typography_font_family": "Montserrat",
                "typography_font_size": {
                  "unit": "px",
                  "size": 56,
                  "sizes": {
                    "mobile": 32,
                    "tablet": 42
                  }
                },
                "typography_font_weight": "700",
                "typography_line_height": {
                  "unit": "em",
                  "size": 1.2
                },
                "text_shadow_text_shadow_type": "yes",
                "text_shadow_text_shadow": {
                  "horizontal": 0,
                  "vertical": 2,
                  "blur": 10,
                  "color": "rgba(0,0,0,0.5)"
                }
              }
            },
            {
              "id": "hero-subheading",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Iconic Imagery for Iconic Brands with an Edge",
                "align": "center",
                "header_size": "h2",
                "title_color": "#f0f0f0",
                "typography_typography": "custom",
                "typography_font_family": "Open Sans",
                "typography_font_size": {
                  "unit": "px",
                  "size": 24,
                  "sizes": {
                    "mobile": 16,
                    "tablet": 20
                  }
                },
                "typography_font_weight": "300",
                "typography_line_height": {
                  "unit": "em",
                  "size": 1.5
                },
                "margin": {
                  "top": "20",
                  "bottom": "40",
                  "unit": "px"
                }
              }
            },
            {
              "id": "hero-buttons",
              "elType": "widget",
              "widgetType": "button",
              "settings": {
                "text": "Book Your Free Consultation",
                "align": "center",
                "button_type": "primary",
                "size": "lg",
                "button_text_color": "#ffffff",
                "background_color": "#d4af37",
                "button_background_hover_color": "#b8941f",
                "typography_typography": "custom",
                "typography_font_family": "Montserrat",
                "typography_font_size": {
                  "unit": "px",
                  "size": 18
                },
                "typography_font_weight": "600",
                "border_radius": {
                  "unit": "px",
                  "size": 3
                },
                "button_box_shadow_box_shadow_type": "yes",
                "button_box_shadow_box_shadow": {
                  "horizontal": 0,
                  "vertical": 4,
                  "blur": 15,
                  "spread": 0,
                  "color": "rgba(212,175,55,0.4)"
                },
                "padding": {
                  "top": "18",
                  "right": "45",
                  "bottom": "18",
                  "left": "45",
                  "unit": "px"
                },
                "margin": {
                  "bottom": "20",
                  "unit": "px"
                }
              }
            },
            {
              "id": "hero-button-secondary",
              "elType": "widget",
              "widgetType": "button",
              "settings": {
                "text": "View Portfolio",
                "align": "center",
                "button_type": "secondary",
                "size": "lg",
                "button_text_color": "#ffffff",
                "background_color": "transparent",
                "border_border": "solid",
                "border_width": {
                  "top": "2",
                  "right": "2",
                  "bottom": "2",
                  "left": "2",
                  "unit": "px"
