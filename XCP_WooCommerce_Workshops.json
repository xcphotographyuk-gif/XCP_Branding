      "elements": [
        {
          "id": "street_col1",
          "elType": "column",
          "settings": {
            "width": {"unit": "%", "size": 50},
            "width_tablet": {"unit": "%", "size": 100}
          },
          "elements": [
            {
              "id": "street_heading",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "City Walk Street Photography",
                "header_size": "h3",
                "title_color": "#1A1A1A",
                "typography_font_size": {"unit": "px", "size": 34}
              }
            },
            {
              "id": "street_description",
              "elType": "widget",
              "widgetType": "text-editor",
              "settings": {
                "editor": "<p style='color: #555; font-size: 17px; line-height: 1.8; margin-bottom: 25px;'>Explore Brighton's vibrant streets and learn to capture authentic moments. This hands-on workshop covers composition, timing, approaching subjects, and finding your unique perspective.</p><p style='color: #333; font-size: 16px; line-height: 1.7;'><strong>What You'll Learn:</strong><br>• Street composition techniques<br>• Working with natural light<br>• Camera settings for candid photography<br>• Ethical street photography practices<br>• Post-processing your best shots</p>",
                "align": "left"
              }
            }
          ]
        },
        {
          "id": "street_col2",
          "elType": "column",
          "settings": {
            "width": {"unit": "%", "size": 50},
            "width_tablet": {"unit": "%", "size": 100}
          },
          "elements": [
            {
              "id": "street_pricing",
              "elType": "widget",
              "widgetType": "icon-box",
              "settings": {
                "icon": {"value": "fas fa-users", "library": "fa-solid"},
                "icon_color": "#1A1A1A",
                "icon_size": {"unit": "px", "size": 42},
                "title_text": "Group Workshop - From £1,500",
                "description_text": "Full-day intensive training\n4-10 participants\nProfessional guidance\nHands-on practice\nPost-processing techniques\nPricing varies by venue, numbers, and requirements",
                "title_color": "#1A1A1A",
                "description_color": "#666666",
                "content_vertical_alignment": "top"
              }
            },
            {
              "id": "street_pricing_private",
              "elType": "widget",
              "widgetType": "icon-box",
              "settings": {
                "icon": {"value": "fas fa-user", "library": "fa-solid"},
                "icon_color": "#C9A961",
                "icon_size": {"unit": "px", "size": 42},
                "title_text": "Private Workshop - Custom Pricing",
                "description_text": "Fully customized curriculum\nOne-on-one intensive training\nLocation of your choice\nPost-workshop review\nOngoing mentorship\nPricing tailored to your specific needs",
                "title_color": "#1A1A1A",
                "description_color": "#666666",
                "content_vertical_alignment": "top"
              }
            }
          ]
        }
