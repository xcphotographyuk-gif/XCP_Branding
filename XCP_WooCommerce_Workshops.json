    {
      "id": "workshops_faq",
      "elType": "section",
      "settings": {
        "layout": "boxed",
        "content_width": {"unit": "px", "size": 1000},
        "padding": {"unit": "px", "top": 70, "right": 20, "bottom": 70, "left": 20},
