        "background_background": "classic",
        "background_image": {"url": "[uc_local]assets/images/workshops/workshops-hero.jpg"},
        "background_position": "center center",
        "background_size": "cover",
        "background_overlay_background": "classic",
        "background_overlay_color": "rgba(0,0,0,0.6)",
        "content_position": "middle",
        "padding": {"unit": "px", "top": 100, "right": 20, "bottom": 100, "left": 20},
        "_title": "Workshops Hero"
      },
      "elements": [
        {
          "id": "workshops_hero_col",
          "elType": "column",
          "settings": {
            "width": {"unit": "%", "size": 100}
          },
          "elements": [
            {
              "id": "workshops_hero_heading",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Photography Workshops & Courses",
                "header_size": "h1",
                "align": "center",
                "title_color": "#FFFFFF",
                "typography_font_size": {"unit": "px", "size": 58},
                "typography_font_size_tablet": {"unit": "px", "size": 44},
                "typography_font_size_mobile": {"unit": "px", "size": 36}
              }
            },
            {
              "id": "workshops_hero_subtitle",
              "elType": "widget",
              "widgetType": "text-editor",
              "settings": {
                "editor": "<p style='text-align: center; color: #FFFFFF; font-size: 24px; max-width: 800px; margin: 25px auto; line-height: 1.6;'>Master your craft with hands-on workshops in street, music, and studio photography. Book your spot and elevate your skills today.</p>",
                "align": "center"
              }
            }
          ]
        }
      ]
    },
    {
      "id": "workshops_intro",
      "elType": "section",
      "settings": {
        "layout": "boxed",
        "content_width": {"unit": "px", "size": 1000},
        "padding": {"unit": "px", "top": 70, "right": 20, "bottom": 50, "left": 20},
        "background_background": "classic",
        "background_color": "#FFFFFF",
        "_title": "Introduction"
      },
      "elements": [
        {
          "id": "intro_col",
          "elType": "column",
          "settings": {
            "width": {"unit": "%", "size": 100}
          },
          "elements": [
            {
              "id": "intro_text",
              "elType": "widget",
              "widgetType": "text-editor",
              "settings": {
                "editor": "<p style='text-align: center; color: #333; font-size: 19px; max-width: 750px; margin: 0 auto; line-height: 1.8;'>Whether you're just starting out or looking to refine your techniques, our workshops combine expert instruction with real-world practice. Choose from group sessions for collaborative learning or private 1-to-1 instruction tailored to your goals.</p>",
                "align": "center"
              }
            }
          ]
        }
      ]
    },
    {
      "id": "woo_workshops_section",
      "elType": "section",
      "settings": {
        "layout": "boxed",
        "content_width": {"unit": "px", "size": 1200},
        "padding": {"unit": "px", "top": 40, "right": 20, "bottom": 70, "left": 20},
        "background_background": "classic",
        "background_color": "#F8F8F8",
        "_title": "WooCommerce Workshop Products"
      },
      "elements": [
        {
          "id": "woo_workshops_col",
          "elType": "column",
          "settings": {
            "width": {"unit": "%", "size": 100}
          },
          "elements": [
            {
              "id": "woo_workshops_heading",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Available Workshops",
                "header_size": "h2",
                "align": "center",
                "title_color": "#1A1A1A",
                "typography_font_size": {"unit": "px", "size": 40}
              }
            },
            {
              "id": "woo_workshops_widget",
              "elType": "widget",
              "widgetType": "woocommerce-products",
              "settings": {
                "rows": 2,
                "columns": 3,
                "columns_tablet": 2,
                "columns_mobile": 1,
                "orderby": "menu_order",
                "query_post_type": "product",
                "query_include_by": "terms",
                "query_include_term_ids": "<!-- REPLACE WITH: Workshops Category ID -->",
                "show_rating": true,
                "show_result_count": false,
                "_element_note": "Replace query_include_term_ids with your WooCommerce category ID for Workshops after creating products"
              }
            }
          ]
        }
      ]
    },
    {
      "id": "street_photography_section",
      "elType": "section",
      "settings": {
        "layout": "boxed",
        "content_width": {"unit": "px", "size": 1200},
